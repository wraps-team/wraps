{
  "name": "byo",
  "version": "0.0.0",
  "private": true,
  "description": "Bring Your Own Cloud - Deploy email infrastructure to your AWS account",
  "repository": {
    "type": "git",
    "url": "https://github.com/byo-team/byo.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "lint": "npx ultracite@latest check",
    "typecheck": "turbo run typecheck",
    "check": "npx ultracite@latest check",
    "fix": "npx ultracite@latest fix",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "turbo": "^2.2.3",
    "ultracite": "6.3.2"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.0.0"
}
