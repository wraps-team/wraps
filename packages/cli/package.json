{
  "name": "@wraps.dev/cli",
  "version": "0.3.3",
  "description": "CLI for deploying Wraps email infrastructure to your AWS account",
  "type": "module",
  "main": "./dist/cli.js",
  "bin": {
    "wraps": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wraps-team/wraps.git",
    "directory": "packages/cli"
  },
  "homepage": "https://wraps.dev",
  "bugs": {
    "url": "https://github.com/wraps-team/wraps/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "pnpm build:console && pnpm build:lambda && tsup",
    "build:lambda": "tsx scripts/build-lambda.ts",
    "build:console": "pnpm --filter @wraps/console build",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "aws",
    "ses",
    "email",
    "infrastructure",
    "cli"
  ],
  "author": "Wraps",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.490.0",
    "@aws-sdk/client-cloudwatch": "^3.490.0",
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-iam": "3.932.0",
    "@aws-sdk/client-lambda": "3.925.0",
    "@aws-sdk/client-mailmanager": "3.925.0",
    "@aws-sdk/client-route-53": "3.925.0",
    "@aws-sdk/client-ses": "^3.490.0",
    "@aws-sdk/client-sesv2": "3.925.0",
    "@aws-sdk/client-sns": "^3.490.0",
    "@aws-sdk/client-sts": "^3.490.0",
    "@aws-sdk/util-dynamodb": "3.927.0",
    "@clack/prompts": "^0.11.0",
    "@pulumi/aws": "^6.20.0",
    "@pulumi/pulumi": "^3.100.0",
    "args": "^5.0.3",
    "cosmiconfig": "^9.0.0",
    "esbuild": "^0.25.12",
    "express": "^4.21.2",
    "get-port": "^7.1.0",
    "http-terminator": "^3.2.0",
    "isomorphic-dompurify": "2.32.0",
    "mailparser": "3.9.0",
    "open": "^10.1.0",
    "picocolors": "^1.1.1",
    "tabtab": "^3.0.2"
  },
  "devDependencies": {
    "@types/args": "5.0.4",
    "@types/express": "^5.0.0",
    "@types/mailparser": "3.4.6",
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "4.0.7",
    "aws-sdk-client-mock": "4.1.0",
    "aws-sdk-client-mock-vitest": "7.0.1",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "tsx": "4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
